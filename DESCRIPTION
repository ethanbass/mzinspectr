Package: mzinspectr
Type: Package
Title: Read and Analyze Mass Spectrometry Alignment Files 
Version: 0.4.2
Authors@R: 
    person("Ethan", "Bass", , "ethanbass@gmail.com", role = c("aut", "cre"),
